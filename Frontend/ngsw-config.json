{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "src/styles.css"
        ],
        "urls": [
          "https://cdnjs.cloudflare.com/**",
          "http://openweathermap.org/**",
          "https://fonts.googleapis.com/**"

        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
"dataGroups": [
    {
      "name": "api-data",
      "urls": [
        "/**"
    ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 200,
        "maxAge": "1h",
        "timeout": "5s"
      }
    }
  ]
}
